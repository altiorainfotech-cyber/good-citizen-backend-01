{
  "version": 2,
  "buildCommand": "cd goodcitizennestjs-main && npm install && npm run build",
  "installCommand": "cd goodcitizennestjs-main && npm install",
  "builds": [
    {
      "src": "goodcitizennestjs-main/dist/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "goodcitizennestjs-main/dist/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
