{
  "name": "ambulanceapp",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc -p tsconfig.build.json --skipLibCheck",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "serve": "vercel dev",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:pbt": "jest --testNamePattern=\"Property\" --verbose",
    "test:unit": "jest --testNamePattern=\"^(?!.*Property).*$\" --verbose",
    "seed:database": "ts-node -r tsconfig-paths/register src/migration/scripts/seed-database.ts",
    "seed:healthcare": "ts-node -r tsconfig-paths/register src/migration/scripts/seed-database.ts --skip-achievements --skip-payment-methods",
    "seed:achievements": "ts-node -r tsconfig-paths/register src/migration/scripts/seed-database.ts --skip-healthcare --skip-payment-methods",
    "seed:payment-methods": "ts-node -r tsconfig-paths/register src/migration/scripts/seed-database.ts --skip-healthcare --skip-achievements",
    "migrate:legacy": "ts-node -r tsconfig-paths/register src/migration/scripts/run-migration.ts",
    "validate:database": "ts-node -r tsconfig-paths/register src/migration/scripts/seed-database.ts --validate-only"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.14.0",
    "@aws-sdk/client-s3": "^3.812.0",
    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "@mapbox/polyline": "^1.2.1",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.1",
    "@nestjs/platform-socket.io": "^11.0.11",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.0.6",
    "@nestjs/websockets": "^11.0.11",
    "@socket.io/redis-adapter": "^8.3.0",
    "@turf/turf": "^7.2.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/uuid": "^10.0.0",
    "auth0": "^4.37.0",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "buffer-equal-constant-time": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "file-type": "^16.5.4",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.3",
    "jwa": "^2.0.1",
    "mime-types": "^3.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.12.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nestjs-firebase": "^11.0.1",
    "nestjs-sharp": "^1.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.2",
    "@types/file-type": "^10.6.0",
    "@types/geojson": "^7946.0.16",
    "@types/jest": "^29.5.14",
    "@types/mapbox__polyline": "^1.0.5",
    "@types/mime-types": "^3.0.1",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "fast-check": "^3.15.1",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "useESM": false
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^file-type$": "<rootDir>/../__mocks__/file-type.ts",
      "^uuid$": "<rootDir>/../__mocks__/uuid.ts"
    }
  }
}
